html
  head
    title index
    link(rel='stylesheet', href='/static/css/default.css') 
    style
      .container{
        margin:40 auto;
        width:800px;
        height:350px;
        border:1px solid #f0f0f0;
      }
      .container #stock{
        margin-top:5px;
      }
      .selectTime{
        overflow:hidden;
        position:absolute;
        font-size:10px;
        width:567px;
        margin:5px 0px;
      }
      #select{
        overflow:hidden;
        float:left;
        margin:0px 0px;
        padding:0px 0px;
      }
      #select li{
        float:left;
        border:1px solid #f2f2f2;
        padding:3px 5px;
        cursor:pointer;
      }
      #select li.selected{
        background:#f1f1f1;
      }
      .selectTime #timeInterval{
        float:right;
      }
      .selectTime #timeInterval input{
        width:80px;
        height:20px;
        color:#888888;
      }
      #compareStock{
        font-size:12px;
        overflow:hidden;
      }
      #compareStock #stockInput{
        width:80px;
      } 
      #chartOperation{
        float:right;
        margin-right:3px;
      }
      #chartOperation span{
        line-height:25px;
        margin-left:5px;
      }

      #cutSVG{
        margin:auto;
        border:1px solid black;
        background:#f1f1f1;
        width:40px;
        cursor:pointer;
      }
      .error{
        border:1px solid red;
      }
      label {
        display:block;
        float:left;
        padding-right:10px;
        white-space:nowrap;
      }
      input[type="checkbox"] {
        vertical-align:middle;
      }
      label span{
        vertical-align:middle;
      }
      .compareList{
        overflow:hidden;
        display:inline-block;
        margin-bottom:-5px;
      }
  body
